<core:FragmentDefinition
    xmlns:mvc="sap.ui.core.mvc"
    xmlns="sap.m"
    xmlns:cards="sap.f.cards"
    xmlns:core="sap.ui.core"
    xmlns:f="sap.f"
    xmlns:tnt="sap.tnt"
    xmlns:form="sap.ui.layout.form"
    xmlns:semantic="sap.m.semantic"
    xmlns:u="sap.ui.unified"
    xmlns:html="http://www.w3.org/1999/xhtml"
    xmlns:layout="sap.ui.layout"
    displayBlock="true"
    height="100%"
>
    <Label id="_IDGenLabel34" />
    <form:SimpleForm
        id="idReviewAccType"
        layout="ResponsiveGridLayout"
        labelSpanXL="4"
        labelSpanL="3"
        labelSpanM="4"
        labelSpanS="12"
        adjustLabelSpan="false"
        emptySpanXL="0"
        emptySpanL="4"
        emptySpanM="0"
        emptySpanS="0"
        columnsXL="2"
        columnsL="1"
        columnsM="1"
        singleContainerFullSize="false"
        ariaLabelledBy="ariaTitle1"
    >
        <form:toolbar>
            <Toolbar id="_IDGenToolbar1">
                <Title
                    id="_IDGenTitle1"
                    text="1. Select access type"
                />
                <ToolbarSpacer id="_IDGenToolbarSpacer3" />
                <Button
                    id="_IDGenButton10"
                    icon="sap-icon://edit"
                    text="Edit"
                    press="handleGoToAccessType"
                />
            </Toolbar>
        </form:toolbar>
        <form:content>
            <Label
                id="_IDGenLabel36"
                text="Type of access"
            />
            <Text text="{wizardModel>/accessRequest}" />
        </form:content>
    </form:SimpleForm>
    <Label id="_IDGenLabel35" />
    <form:SimpleForm
        id="idReviewCompInfo"
        layout="ResponsiveGridLayout"
        labelSpanXL="4"
        labelSpanL="3"
        labelSpanM="4"
        labelSpanS="12"
        adjustLabelSpan="false"
        emptySpanXL="0"
        emptySpanL="4"
        emptySpanM="0"
        emptySpanS="0"
        columnsXL="2"
        columnsL="1"
        columnsM="1"
        singleContainerFullSize="false"
        ariaLabelledBy="ariaTitle1"
    >
        <form:toolbar>
            <Toolbar id="_IDGenToolbar2">
                <Title
                    id="_IDGenTitle11"
                    text="2. Requestor/Company Information"
                />
                <ToolbarSpacer id="_IDGenToolbarSpacer4" />
                <Button
                    id="_IDGenButton11"
                    icon="sap-icon://edit"
                    text="Edit"
                    press="handleGoToCompanyInfo"
                />
            </Toolbar>
        </form:toolbar>
        <form:content>
            <Label
                id="_IDGenLabel37"
                text="Type of request"
            />
            <Text text="{wizardModel>/requestType}" />
            <Label id="_IDGenLabel38" />
            <Title
                id="_IDGenTitle14"
                text="Requestor Information"
            />
            <Label
                id="_IDGenLabel39"
                text="First Name"
            />
            <Text
                id="_IDGenText"
                text="{wizardModel>/firstName}"
            />
            <Label
                id="_IDGenLabel40"
                text="Last Name"
            />
            <Text
                id="_IDGenText1"
                text="{wizardModel>/lastName}"
            />
            <Label
                id="_IDGenLabel41"
                text="Phone Number"
            />
            <Text
                id="_IDGenText2"
                text="{wizardModel>/phone}"
            />
            <Label
                id="_IDGenLabel42"
                text="Email Address"
            />
            <Text
                id="_IDGenText3"
                text="{wizardModel>/email}"
            />
            <Label id="_IDGenLabel45" />
            <Title
                id="_IDGenTitle15"
                text="Employer company information"
            />
            <Label
                id="_IDGenLabel43"
                text="Employer Name"
            />
            <Text
                id="_IDGenText4"
                text="{wizardModel>/employerName}"
            />
            <Label
                id="_IDGenLabel44"
                text="{wizardModel>/employerName}"
            />
            <Text
                id="_IDGenText5"
                text="500, Toronto"
            />
            <Label id="_IDGenLabel46" />
            <Title
                id="_IDGenTitle16"
                text="Authorized signatory"
            />
            <Label
                id="_IDGenLabel47"
                text="Name"
            />
            <Text
                id="_IDGenText6"
                text="{wizardModel>/firstName} {wizardModel>/lastName}"
            />
            <Label
                id="_IDGenLabel48"
                text="Job title"
            />
            <Text
                id="_IDGenText7"
                text="{wizardModel>/empjobTitle}"
            />
            <Label
                id="_IDGenLabel49"
                text="Email"
            />
            <Text
                id="_IDGenText8"
                text="{wizardModel>/email}"
            />
            <Label
                id="_IDGenLabel50"
                text="Phone number"
            />
            <Text
                id="_IDGenText9"
                text="{wizardModel>/phone}"
            />
        </form:content>
    </form:SimpleForm>
    <form:SimpleForm
        id="idSubScriberInfo"
        layout="ResponsiveGridLayout"
        labelSpanXL="4"
        labelSpanL="3"
        labelSpanM="4"
        labelSpanS="12"
        adjustLabelSpan="false"
        emptySpanXL="0"
        emptySpanL="4"
        emptySpanM="0"
        emptySpanS="0"
        columnsXL="2"
        columnsL="1"
        columnsM="1"
        singleContainerFullSize="false"
        ariaLabelledBy="ariaTitle1"
    >
        <form:toolbar>
            <Toolbar id="_IDGenToolbar3">
                <Title
                    id="_IDGenTitle12"
                    text="3. Subscriber information"
                />
                <ToolbarSpacer id="_IDGenToolbarSpacer5" />
                <Button
                    id="_IDGenButton12"
                    icon="sap-icon://edit"
                    text="Edit"
                    press="handleGoToSubscriber"
                />
            </Toolbar>
        </form:toolbar>

        <form:content>
            <Table
                id="idTableAddOper1"
                width="100%"
                mode="None"
                items="{wizardModel>/subscribers}"
            >
                <columns>
                    <Column
                        id="_IDGenColumn"
                        width="16%"
                    >
                        <header>
                            <Label text="First Name" />
                        </header>
                    </Column>
                    <Column
                        id="_IDGenColumn7"
                        width="16%"
                    >
                        <header>
                            <Label text="Last Name" />
                        </header>
                    </Column>
                    <Column
                        id="_IDGenColumn8"
                        width="16%"
                    >
                        <header>
                            <Label text="Job Title" />
                        </header>
                    </Column>
                    <Column
                        id="_IDGenColumn9"
                        width="16%"
                    >
                        <header>
                            <Label text="Email Address" />
                        </header>
                    </Column>
                    <Column
                        id="_IDGenColumn10"
                        width="16%"
                    >
                        <header>
                            <Label text="Phone Number" />
                        </header>
                    </Column>
                    <Column
                        id="_IDGenColumn11"
                        width="16%"
                    >
                        <header>
                            <Label text="Request Effective Date" />
                        </header>
                    </Column>
                </columns>

                <items>
                    <ColumnListItem
                        id="idOperatorAddItem1"
                        type="Active"
                    >
                        <cells>
                            <Text text="{wizardModel>firstName}" />
                            <Text text="{wizardModel>lastName}" />
                            <Text text="{wizardModel>jobTitle}" />
                            <Text text="{wizardModel>email}" />
                            <Text text="{wizardModel>phone}" />
                            <Text text="{wizardModel>requestEffDate}" />
                        </cells>
                    </ColumnListItem>
                </items>
            </Table>
        </form:content>
    </form:SimpleForm>
    <Label id="_IDGenLabel58" />
    <form:SimpleForm
        id="idAccessRights"
        width="100%"
        layout="ResponsiveGridLayout"
    >
        <form:toolbar>
            <Toolbar id="_IDGenToolbar4">
                <Title
                    id="_IDGenTitle13"
                    text="4. Access Rights"
                />
                <ToolbarSpacer id="_IDGenToolbarSpacer6" />
                <Button
                    id="_IDGenButton13"
                    icon="sap-icon://edit"
                    text="Edit"
                    press="handleGoToAccessRights"
                />
            </Toolbar>
        </form:toolbar>

        <form:content>
            <SplitApp id="SplitApp">
                <!-- Master Page (User List) -->
                <masterPages>
                    <Page
                        id="masterPage"
                        title="Users"
                    >
                        <content>
                            <List
                                id="userLists"
                                mode="SingleSelectMaster"
                                items="{AccessModel>/users}"
                                selectionChange="onUserSelect"
                            >
                                <StandardListItem
                                    title="{AccessModel>firstName} {AccessModel>lastName}"
                                />
                            </List>
                        </content>
                    </Page>
                </masterPages>

                <!-- Detail Page (User Access) -->
                <detailPages>
                    <Page
                        id="detailPage"
                        title="User Access"
                    >
                        <content>
                            <VBox id="accessVBox" />
                        </content>
                    </Page>
                </detailPages>
            </SplitApp>
        </form:content>
    </form:SimpleForm>

    <form:SimpleForm>
        <form:content>
        <FlexBox class="reviewPageinfo" id="flexboxreviewpage">
                        <VBox
                            id="_IDGenVBox5"
                            class="sapUiSmallMargin"
                        >
                            <Title
                                id="_IDGenTitle18"
                                text="Legal"
                                level="H4"
                            />
                            <Text
                                text="By submitting this request you certify that all information provided in the request is accurate
       and the Authorized Signatory identified within the request has the authority to bind 2022166 Ontario Inc.
        It is the responsibility of 2022156 Ontario Inc to inform Enbridge Gas Inc. when a change to any access granted is required"
                            />
                        </VBox>
        </FlexBox>
            
        </form:content>
    </form:SimpleForm>
 <form:SimpleForm>
        <form:content>
            <FlexBox
                justifyContent="Start"
                id="captcha"
                height="100px"
            >
                <core:HTML
                    id="recaptchaContainer"
                    content='&lt;div id=&quot;recaptchaGoogle&quot; class=&quot;g-recaptcha&quot; data-sitekey=&quot;6LfaKP8qAAAAAC6ivR0QAFzlnJwj_lKRc-gUxw2a&quot;&gt;&lt;/div&gt;'
                />
            </FlexBox>
        </form:content>
    </form:SimpleForm>

    <HBox
        justifyContent="Start"
        class="sapUiContentPadding"
    >
        <VBox>
            <Button
                text="Submit"
                press="onSubmit"
            />
        </VBox>
    </HBox>
</core:FragmentDefinition>
