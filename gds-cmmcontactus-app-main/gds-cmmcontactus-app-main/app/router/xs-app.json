{
  "routes": [
    {
      "source": "^/noauthcontactus/(.*)",
      "target": "$1",
      "service": "html5-apps-repo-rt",
      "authenticationType": "none"
    },
    {
      "source": "^/(.*)$",
      "target": "$1",
      "destination": "srv-api",
      "csrfProtection": true,
      "authenticationType": "none"
    }
  ],
  "welcomeFile": "/noauthcontactus"
}
